<!DOCTYPE html>
<html lang="en">

<% include head %>

<body class="homebody">
<div class= "header">
	<img src="/images/mainlogo.png" class="title">
</div>
<div class= "body">
	<div class= "container">

		<form name="login" action="/auth/login" method="post">
			<img src="/images/bird.png" class="logo">
			<div class="logindiv">
			<img src="/images/email.png" class="login">
			<input type="text" id="email" name="email" value="email" onclick="empty(this.id)" required>
			</div>
			<div class="logindiv">
			<img src="/images/pw.png" class="login">
			<input type="password" id="password" name="password" value="password" onclick="empty(this.id)" required>
			</div>
			<input type="submit" class="loginButton" value="Log In">
		</form>
	</div>
</div

<% include footer %>

</body>
</html>